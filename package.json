{
  "name": "hyperact",
  "version": "0.0.16",
  "description": "Hyperactive animation layers",
  "main": "hyperact.js",
  "module": "hyperact.mjs",
  "scripts": {
    "build": "./node_modules/.bin/rollup -c && BABEL_ENV=webpack WEBPACK_ENV=build webpack --progress --colors",
    "dev": "./node_modules/.bin/rollup -c && BABEL_ENV=webpack WEBPACK_ENV=dev webpack --progress --colors",
    "test": "eslint source/*.js ; eslint test/*.js --rule 'no-undef:0' ; BABEL_ENV=mocha mocha --compilers js:babel-core/register --colors",
    "transform": "eslint test/transform.js --rule 'no-undef:0' && BABEL_ENV=mocha mocha --compilers js:babel-core/register --colors",
    "gcc": "gcc -framework Cocoa -framework QuartzCore -x objective-c -o ./test/objc ./test/objc.m ; ./test/objc ; rm ./test/objc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinDoughty/hyperact.git"
  },
  "keywords": [
    "implicit",
    "animation",
    "layer",
    "additive",
    "relative"
  ],
  "author": "Kevin Doughty",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/KevinDoughty/hyperact/issues"
  },
  "homepage": "https://github.com/KevinDoughty/hyperact#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^4.0.2",
    "eslint": "^3.17.1",
    "mocha": "^3.4.2",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-eslint": "^3.0.0",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
